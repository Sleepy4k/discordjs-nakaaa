{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "baseUrl": "./",
    "paths": {
      "#utils/*": ["utils/*"],
      "#config/*": ["config/*"],
      "#events/*": ["events/*"],
      "#server/*": ["server/*"],
      "#commands/*": ["commands/*"],
      "#handlers/*": ["handlers/*"],
      "#dashboard/*": ["dashboard/*"],
      "#functions/*": ["functions/*"],
      "#slashCommands/*": ["slashCommands/*"],
      "#messageCommands/*": ["messageCommands/*"]
    }
  },
  "include": ["**/*.js"],
  "exclude": ["node_modules", "**/node_modules", "dist"]
}